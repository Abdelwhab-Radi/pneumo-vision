{
  "data_root": "/mnt/a/newlast/project/chest_xray",
  "cache_dir": "./cache",
  "checkpoint_dir": "./checkpoints",
  "log_dir": "./logs",
  "results_dir": "./results",
  "img_size": 256,
  "batch_size": 16,
  "val_ratio": 0.2,
  "color_mode": "rgb",
  "allowed_exts": [
    ".jpg",
    ".jpeg",
    ".png",
    ".bmp",
    ".tif",
    ".tiff"
  ],
  "model_variant": "B3",
  "dropout_rate": 0.4,
  "use_mixed_precision": true,
  "fine_tune_layers": 80,
  "initial_epochs": 30,
  "fine_tune_epochs": 25,
  "initial_lr": 0.0005,
  "fine_tune_lr": 5e-06,
  "min_lr": 1e-08,
  "use_tta": true,
  "tta_augmentations": 10,
  "use_cross_validation": false,
  "cv_folds": 5,
  "use_class_weights": true,
  "label_smoothing": 0.15,
  "augmentation_strength": 1.2,
  "use_advanced_aug": true,
  "use_albumentations": true,
  "seed": 42,
  "mixed_precision_policy": "mixed_float16",
  "force_resplit": false,
  "split_move_files": false,
  "save_model_plots": true,
  "save_predictions": true,
  "verbose": 1,
  "auto_optimize_for_deployment": true,
  "save_deployment_package": true,
  "deployment_formats": [
    "keras",
    "tflite"
  ],
  "tflite_quantize": true
}